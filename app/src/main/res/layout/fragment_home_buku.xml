                                android:scaleType="fitXY"
                                android:src="@drawable/img_pagecontrol"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_pagecontrol" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageEight"
                            imageRes="@{@drawable/img_8}"
                            android:layout_width="@dimen/_132pxh"
                            android:layout_height="@dimen/_70pxh"
                            android:layout_gravity="bottom|start"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_8" />

