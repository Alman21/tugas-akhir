                            android:layout_marginEnd="@dimen/_40pxh"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_2" />
