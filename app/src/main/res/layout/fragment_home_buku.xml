                    <LinearLayout
                        android:id="@+id/linearRowrectanglethirtyeight"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:orientation="horizontal">

                        <FrameLayout
                            android:id="@+id/frameStackrectanglethirtyeight"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_112pxv">

                            <ImageView
                                android:id="@+id/imageRectangleThirtyEight"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle38}"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_111pxv"
                                android:layout_gravity="start"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_rectangle38" />

                            <ImageView
                                android:id="@+id/imageRectangleFortyOneOne"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle41}"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_111pxv"
                                android:layout_gravity="start"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_rectangle41" />
                        </FrameLayout>

                        <ImageView
                            android:id="@+id/imageRectangleThirtyNine"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle41}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle41" />

                        <FrameLayout
                            android:id="@+id/frameStackrectangleforty"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_112pxv"
                            android:layout_marginStart="@dimen/_15pxh">

                            <ImageView
                                android:id="@+id/imageRectangleForty"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle38}"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_111pxv"
                                android:layout_gravity="start"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_rectangle38" />

                            <ImageView
                                android:id="@+id/imageRectangleFortyTwo"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle41}"
