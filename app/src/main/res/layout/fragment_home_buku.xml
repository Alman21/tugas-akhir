                            style="@style/groupStyleamber_200cornerRadius"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|start"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtHaiLulu"
                                style="@style/txtPoppinsbold16"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_143pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_marginEnd="@dimen/_143pxh"
                                android:layout_marginBottom="@dimen/_14pxv"
                                android:ellipsize="end"
                                android:textSize="15dp"
                                android:gravity="center_horizontal"
                                android:lineSpacingExtra="@dimen/_8pxh"
                                android:singleLine="true"
                                android:text="@{homeBukuVM.homeBukuModel.txtHaiLulu}"
                                tools:text="@string/lbl_hai_lulu" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imageStatusBar"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_15pxv"
                            android:layout_gravity="top|start"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_statusbar"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_statusbar" />
                    </FrameLayout>

                    <FrameLayout
