                        android:layout_width="@dimen/_344pxh"
                        android:layout_height="@dimen/_170pxv"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_marginEnd="@dimen/_7pxh">

                        <LinearLayout
                            android:id="@+id/linearColumnten"
                            style="@style/groupStyleamber_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginEnd="@dimen/_14pxh"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageTen"
                                imageRes="@{@drawable/img_10}"
                                android:layout_width="@dimen/_99pxh"
                                android:layout_height="@dimen/_104pxh"
                                android:layout_gravity="start"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginEnd="@dimen/_16pxh"
