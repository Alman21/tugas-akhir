                    <LinearLayout
                        android:id="@+id/linearRowrectanglethirtyfive"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageRectangleThirtyFive"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle35}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle35" />

                        <ImageView
                            android:id="@+id/imageRectangleThirtySix"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle36}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle36" />

                        <ImageView
                            android:id="@+id/imageRectangleThirtySeven"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle37}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle37" />
