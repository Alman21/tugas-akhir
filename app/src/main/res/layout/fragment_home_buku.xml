                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle32" />

                        <ImageView
                            android:id="@+id/imageRectangleThirtyThree"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle33}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle33" />

                        <ImageView
                            android:id="@+id/imageRectangleFortyOne"
                            cornerRadius="@{@dimen/_15pxh}"
                            imageRes="@{@drawable/img_rectangle29}"
                            android:layout_width="@dimen/_96pxh"
                            android:layout_height="@dimen/_111pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_rectangle29" />
                    </LinearLayout>

