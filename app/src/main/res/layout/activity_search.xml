<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchVM" type="com.dessapplication.app.modules.search.data.viewmodel.SearchVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnlineseven"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnstatusbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackkeluaranterbar"
                android:layout_height="@dimen/_66pxv" android:layout_width="match_parent">
                <LinearLayout
                    android:id="@+id/linearColumnkeluaranterbar"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleamber_200cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtKeluaranTerbar"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_107pxh"
                        android:layout_marginStart="@dimen/_108pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:singleLine="true"
                        android:textSize="15dp"
                        android:text="@{searchVM.searchModel.txtKeluaranTerbar}"
                        style="@style/txtPoppinsbold16" tools:text="@string/msg_keluaran_terbar"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageStatusBar"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_15pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_statusbar_8"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_statusbar_8"/>
            </FrameLayout>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_amber_200_radius_15"
                android:gravity="center_horizontal"
                android:id="@+id/searchViewGroupThirtyOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:theme="@style/searchViewGroupThirtyOne"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/msg_keluaran_terbar" app:searchIcon="@drawable/img_menu"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectangletwentynine"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleTwentyNine"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle29_145x125}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle29_145x125"/>
                <ImageView android:id="@+id/imageRectangleThirty"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle30_145x125}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle30_145x125"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowpestapenulis"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPestaPenulis"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.45"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtPestaPenulis}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_pesta_penulis"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMalingKundang"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_63pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.55"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtMalingKundang}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_maling_kundang"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectanglethirtyone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyOne"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle31_145x125}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle31_145x125"/>
                <ImageView android:id="@+id/imageRectangleThirtyTwo"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle32_145x125}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle32_145x125"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcerminajaib"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_62pxh"
                android:layout_marginStart="@dimen/_62pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCerminAjaib"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.65"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtCerminAjaib}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_cermin_ajaib"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSyukur"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_93pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.35"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtSyukur}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_syukur"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectanglethirtythree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyThree"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle33_145x125}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33_145x125"/>
                <ImageView android:id="@+id/imageRectangleThirtyFour"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle34}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle34"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsalingmengharg"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSalingMengharg"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.67"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtSalingMengharg}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/msg_saling_mengharg"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAsalUsul"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtAsalUsul}"
                    style="@style/txtPoppinssemibold12" tools:text="@string/lbl_asal_usul"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackhome"
                android:layout_height="@dimen/_72pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleamber_201">
                    <LinearLayout android:id="@+id/linearColumnhome"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageHome"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_home_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_900"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtHomeOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtHomeOne}"
                            style="@style/txtPoppinslight12_2" tools:text="@string/lbl_home"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnsearchOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageSearchOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_deep_orange_400"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search_deep_orange_400"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtSearchTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtSearchTwo}"
                            style="@style/txtPoppinslight12" tools:text="@string/lbl_search"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnuser"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageUser"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtBook"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtBook}"
                            style="@style/txtPoppinslight12_1" tools:text="@string/lbl_book"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnlock"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageLock"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_27pxh"
                            android:layout_width="@dimen/_27pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_lock"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtProfile}"
                            style="@style/txtPoppinslight12_1" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageLineSeven"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_43pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_line7"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_line7"/>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
